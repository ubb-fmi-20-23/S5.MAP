<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:m="urn:import:gwt.material.design.client.ui"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:demo="urn:import:gwt.material.design.demo.client.ui">
    <m:MaterialPanel>
        <m:MaterialTitle title="Text Only" />
        <m:MaterialNavBar>
            <m:MaterialBreadcrumb text="First"/>
            <m:MaterialBreadcrumb text="Second"/>
            <m:MaterialBreadcrumb text="Third"/>
        </m:MaterialNavBar>
        <demo:PrettyPre addStyleNames="lang-xml z-depth-1">
            &emsp;&lt;m:MaterialNavBar><br/>
            &emsp;&lt;m:MaterialBreadcrumb text="First"/><br/>
            &emsp;&lt;m:MaterialBreadcrumb text="Second"/><br/>
            &emsp;&lt;m:MaterialBreadcrumb text="Third"/><br/>
            &lt;/m:MaterialNavBar><br/>
        </demo:PrettyPre>

        <m:MaterialTitle title="Only Icons" />
        <m:MaterialNavBar>
            <m:MaterialBreadcrumb iconType="POLYMER"/>
            <m:MaterialBreadcrumb iconType="STAR"/>
            <m:MaterialBreadcrumb iconType="MOTORCYCLE"/>
        </m:MaterialNavBar>
        <demo:PrettyPre addStyleNames="lang-xml z-depth-1">
            &emsp;&lt;m:MaterialNavBar><br/>
            &emsp;&lt;m:MaterialBreadcrumb iconType="POLYMER"/><br/>
            &emsp;&lt;m:MaterialBreadcrumb iconType="STAR"/><br/>
            &emsp;&lt;m:MaterialBreadcrumb iconType="MOTORCYCLE"/><br/>
            &lt;/m:MaterialNavBar><br/>
        </demo:PrettyPre>

        <m:MaterialTitle title="Text with Icons" />
        <m:MaterialNavBar>
            <m:MaterialBreadcrumb iconPosition="LEFT" iconType="POLYMER" text="First"/>
            <m:MaterialBreadcrumb iconPosition="LEFT" iconType="STAR" text="Second"/>
            <m:MaterialBreadcrumb iconPosition="LEFT" iconType="MOTORCYCLE" text="Third"/>
        </m:MaterialNavBar>
        <demo:PrettyPre addStyleNames="lang-xml z-depth-1">
            &emsp;&lt;m:MaterialNavBar><br/>
            &emsp;&lt;m:MaterialBreadcrumb iconPosition="LEFT" iconType="POLYMER" text="First"/><br/>
            &emsp;&lt;m:MaterialBreadcrumb iconPosition="LEFT" iconType="STAR" text="Second"/><br/>
            &emsp;&lt;m:MaterialBreadcrumb iconPosition="LEFT" iconType="MOTORCYCLE" text="Third"/><br/>
            &lt;/m:MaterialNavBar><br/>
        </demo:PrettyPre>
    </m:MaterialPanel>
</ui:UiBinder>
